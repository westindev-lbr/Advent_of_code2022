{
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.11",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "type": "module",
  "scripts": {
    "dev": "tsc && node ./build/script.js",
